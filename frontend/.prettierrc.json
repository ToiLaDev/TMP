{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 200,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "crlf",
  "arrowParens": "always",
  "trailingComma": "none",
  "singleAttributePerLine": true,
  "vueIndentScriptAndStyle": false
}